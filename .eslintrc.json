{
    "extends": "airbnb",
    "rules": {
        "indent": ["error", 4],
        "spaced-comment": ["error", "always"],
        "object-shorthand": ["error", "never"],
        "func-names": ["error", "never"],
        "no-var": "off"
    },
    "env": {
        "browser": true
    },
    "globals": {
        "Phaser": true,
        "game": true
    }
}